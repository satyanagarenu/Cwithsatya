#include<stdio.h>
int main(){
	int a;
	printf("enter the year");
	scanf("%d",&a);
	if(a%4==0){
		if(a%100==0){
			if(a%400==0){
			
			printf(" the leap year");
				
		}
		else{
			printf("not a leap year");
			
		}
	}
	
	if(a%4==0&&a%100!=0){
		printf(" aleap year");
	}
	
}
	else{
			printf(" not a leap year");
		}
		return 0;
	
}
